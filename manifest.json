{
    "_version" : "1.1.0",
    "sap.app" : {
        "_version" : "1.1.0",
        "id" : "org.mah.app",
        "type" : "application",
        "i18n" : "i18n/i18n.properties",
        "title" : "{{appTitle}}",
        "description" : "{{appDescription}}",
        "applicationVersion" : {
            "version" : "1.0.0"
        },

        "dataSources" : {
            "employee" : {
                "uri" : "/models/json/Employee.json",
                "type" : "JSON"
            }
        }
    },

    "sap.ui" : {

    },

    "sap.ui5" : {
        "_version" : "1.1.0",
        "rootView" : "org.mah.app.view.Employee",
        "models" : {
            "i18n" : {
                "type" : "sap.ui.model.resource.ResourceModel",
                "settings" : {
                    "bundleName" : "org.mah.app.i18n.i18n"
                }
            },

            "employee" : {
                "dataSource" : "employee"
            }
        }
    }
}